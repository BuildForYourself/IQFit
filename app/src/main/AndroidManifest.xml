<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.buildforyourself.iqfit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".IQFitApplication">
        <activity
            android:name=".StackActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FoodCategoriesActivity"
            android:label="Food Categories"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".FoodComponentsActivity"
            android:label="Food Components"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".ChartTabActivity"
            android:label="@string/title_activity_chart_tab"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>